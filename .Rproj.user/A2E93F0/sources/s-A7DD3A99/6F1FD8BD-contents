set.seed(1)
dat <- TAR1(t = 100, burnin = 20,
            u_w = 0, u_h = 0, phi_w = 0.5, phi_h = 0.5,
            type_w = "bilinear", type_h = "linear",
            betas_y = c(0.4, 0.1), betas_x = 0.2,
            k_w = -0.1)


plot(dat[dat$Partner == "x", "Behavior"], dat[dat$Partner == "x", "Influence"])
